<section class="data">
  <h3>Receipt info</h3>

  <div class="data-container">
    {% for receipt in receipts%}
      <section class="data-folder">
        <div>
          <span class="data-content">
            <p class="data-name">Start</p>
            <p>{{ payment.startAt | date("d/m/Y") }}</p>
          </span>

          <span class="data-content">
            <p class="data-name">end</p>
            <p>{{ payment.endAt | date("d/m/Y") }}</p>
          </span>
        </div>
        <div>
          {#          <a href="{{ path('',{'id':})}}">See</a>#}
        </div>
      </section>
    {% endfor %}
  </div>
</section>
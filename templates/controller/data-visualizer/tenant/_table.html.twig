<div class="table" id="tenant-table">
  <div class="table-head">
        <span class="ligne-content">
            {% for head in heads %}
              <p class="th">{{ head }}</p>
            {% endfor %}
        </span>
  </div>
  <div class="table-body">
    {% for tenant in data %}
      <span class="ligne-content">
                <p class="td">{{ tenant.name }}</p>
                <p class="td">{{ tenant.lastname }}</p>
                <p class="td">{{ tenant.email }}</p>
                <p class="td">{{ tenant.address }}</p>
                <p class="td">{{ tenant.phone }}</p>
                <p class="td">{{ tenant.apl ? 'oui': 'non' }}</p>
                <p class="td">{{ tenant.aplValue }}</p>
                <a class="td" href="{{ path('app_tenant_selected',{'id':tenant.id}) }}">
                    <span class="btn">
                        sÃ©lect
                    </span>
                </a>
            </span>
    {% endfor %}
  </div>
</div>
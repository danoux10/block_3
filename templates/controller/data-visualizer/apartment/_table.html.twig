<div class="table" id="apartment-table">
  <div class="table-head">
        <span class="ligne-content">
{#            {% for head in heads %}#}
              <p class="th">code postal</p>
              <p class="th">ville</p>
              <p class="th">addresse</p>
              <p class="th">charge</p>
              <p class="th">garantie</p>
              <p class="th">loyer</p>
              <p class="th">select</p>
{#            {% endfor %}#}
        </span>
  </div>
  <div class="table-body">
    {% for apartment in data %}
      <span class="ligne-content">
{#        <p class="td">{{ apartment.id }}</p>#}
        <p class="td">{{ apartment.code }}</p>
        <p class="td">{{ apartment.city }}</p>
        <p class="td">{{ apartment.address }}</p>
        <p class="td">{{ apartment.charge }} €</p>
        <p class="td">{{ apartment.guarantee }} €</p>
        <p class="td">{{ apartment.rent }} €</p>
        <a class="td" href="{{ path('app_apartment_selected',{'id':apartment.id}) }}">
          <span class="btn">
            sélect
          </span>
        </a>
      </span>
    {% endfor %}
  </div>
</div>
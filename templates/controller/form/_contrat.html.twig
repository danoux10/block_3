{{ form_start(form_contract,{'attr':{'id':'contract'}}) }}
<h3>Ajouter {{ page_name }}</h3>
<div class="field-content">
    <span class="form-element">
      <span class="label required">
        {{ (form_label(form_contract.start_at)) }}
      </span>
      <span class="input">
        {{ form_widget(form_contract.start_at) }}
      </span>
    </span>

  <span class="form-element">
      <span class="label">
        {{ (form_label(form_contract.end_at)) }}
      </span>
      <span class="input">
        {{ form_widget(form_contract.end_at) }}
      </span>
    </span>

  <span class="form-element">
      <span class="label">
        {{ (form_label(form_contract.Apartment)) }}
      </span>
      <span class="input">
        {{ form_widget(form_contract.Apartment) }}
      </span>
    </span>

  <span class="form-element">
      <span class="label">
        {{ (form_label(form_contract.Tenant)) }}
      </span>
      <span class="input">
        {{ form_widget(form_contract.Tenant) }}
      </span>
    </span>
</div>

{% include'controller/form/utility/_form_submit_required.html.twig' %}
{{ form_end(form_contract) }}
